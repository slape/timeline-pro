{
  "name": "monday-react-quickstart-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run stop && concurrently \"npm run server\" \"npm run expose\"",
    "build": "vite build",
    "expose": "mapps tunnel:create -p 8301",
    "server": "vite",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "stop": "kill-port 8301 && kill-port 4049 && kill-port 4040",
    "lint": "eslint .",
  "lint:fix": "eslint . --fix",
  "format": "prettier --write ."
  },
  "dependencies": {
    "@mondaycom/apps-sdk": "^2.1.2",
    "@vibe/core": "^3.54.2",
    "html-to-image": "^1.11.13",
    "leader-line": "^1.0.8",
    "lodash.isequal": "^4.5.0",
    "moment": "^2.30.1",
    "monday-sdk-js": "^0.5.5",
    "react-rnd": "^10.5.2",
    "validator": "^13.15.15",
    "zustand": "^5.0.7"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@mondaycom/apps-cli": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.3.2",
    "concurrently": "^5.2.0",
    "cross-port-killer": "^1.2.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.54.8",
    "typescript-eslint": "^8.39.1",
    "vite": "^6.2.2"
  },
  "type": "module"
}
